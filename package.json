{
  "name": "country-authority-etl",
  "version": "0.0.0",
  "author": "",
  "private": true,
  "license": "ISC",
  "engines": {
    "node": ">= 22.0.0",
    "npm": ">= 10.7.0",
    "yarn": "Please use NPM!"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json --noEmit && swc src -d dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start -b swc",
    "start:dev": "nest start -b swc -w",
    "start:debug": "nest start --debug -b swc --watch",
    "start:prod": "node --enable-source-maps --stack-size=1024 dist/src/main",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:quiet": "npm run lint -- --quiet",
    "typeorm": "ts-node -r tsconfig-paths/register --project ./tsconfig.json ./node_modules/typeorm/cli.js",
    "typeorm:db": "npm run typeorm -- -d ./src/db/database-local.config.ts",
    "typeorm:ex": "ts-node -r tsconfig-paths/register --project ./tsconfig.json ./node_modules/typeorm-extension/bin/cli.cjs -d ./src/db/database-local.config.ts",
    "migration:run": "npm run typeorm:db schema:sync",
    "migration:generate": "npm run typeorm:db migration:generate ./src/db/migrations/new",
    "migration:create": "npm run typeorm -- migration:create ./src/db/migrations/new",
    "migration:revert": "npm run typeorm:db migration:revert",
    "migration:show": "npm run typeorm:db migration:show",
    "ts:check": "tsc -p ./tsconfig.json --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:db:setup": "NODE_ENV=test npm run typeorm:ex -- db:drop && NODE_ENV=test npm run typeorm:ex -- db:create && NODE_ENV=test npm run migration:run",
    "test:e2e": "jest --config ./test/jest-e2e.ts --coverage --runInBand --detectOpenHandles",
    "ci:test": "npm run test && npm run test:db:setup && test:e2e",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@fastify/static": "^7.0.4",
    "@fastify/swagger": "^8.14.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-fastify": "^10.3.8",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/typeorm": "^10.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "config": "^3.3.11",
    "mssql": "10.0.2",
    "mysql": "^2.18.1",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.5.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.7",
    "@swc/jest": "^0.2.36",
    "@trading-config-pro/eslint-config": "0.0.3",
    "@tsconfig/node22": "22.0.0",
    "@types/config": "^3.3.4",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "husky": "9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "15.2.7",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.1.3",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.4.5"
  }
}
